# This is Git's per-user configuration file.
[user]
    name = USERNAME
    email = USERNAME.git@gmail.com
[core]
    editor = vim
    excludesfile = HOME/.gitignore_global
    fileMode = false

[diff]
    tool = vimdiff
[difftool]
    prompt = false
[alias]
    dif = difftool

[merge]
    tool = vimdiff
[mergetool]
    prompt = false

